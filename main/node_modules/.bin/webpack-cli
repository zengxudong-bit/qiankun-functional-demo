#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../.store/webpack-cli@5.0.1/node_modules/webpack-cli/bin/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../.store/webpack-cli@5.0.1/node_modules/webpack-cli/bin/cli.js" "$@"
  ret=$?
fi
exit $ret
