#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../../nanoid@3.3.4/node_modules/nanoid/bin/nanoid.cjs" "$@"
  ret=$?
else 
  node  "$basedir/../../../../../nanoid@3.3.4/node_modules/nanoid/bin/nanoid.cjs" "$@"
  ret=$?
fi
exit $ret
